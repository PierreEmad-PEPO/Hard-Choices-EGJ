<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:GroupBox name="Container" style="top: 80%; flex-grow: 0; height: 20%; width: auto; background-color: rgb(214, 181, 128); flex-direction: row; flex-wrap: wrap;">
        <ui:VisualElement name="CharAvatar" style="flex-grow: 0; width: 75px; height: 100%; justify-content: flex-start; align-items: center; align-self: flex-start; flex-shrink: 0; margin-left: 0; margin-top: 0; margin-right: 0; margin-bottom: 0; padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px; background-color: rgba(255, 255, 255, 0.51);" />
        <ui:GroupBox name="TextContainer" style="flex-wrap: wrap; flex-grow: 1; flex-shrink: 1; align-items: flex-start; justify-content: flex-start; align-self: auto;">
            <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" name="CharName" style="margin-left: 25px; color: rgb(0, 0, 0);" />
            <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Message" style="flex-grow: 1; flex-direction: row; flex-wrap: wrap; flex-shrink: 1; align-items: auto; align-self: auto; white-space: normal; text-overflow: clip; -unity-font-style: normal; color: rgb(0, 0, 0);" />
        </ui:GroupBox>
    </ui:GroupBox>
</ui:UXML>
